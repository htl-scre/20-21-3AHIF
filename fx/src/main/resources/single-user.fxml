<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<GridPane  maxHeight="80.0" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
          prefHeight="80.0" xmlns:fx="http://javafx.com/fxml/1"
          xmlns="http://javafx.com/javafx/11.0.1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="80.0" prefHeight="80.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <ImageView fx:id="imageView" fitHeight="80.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true"/>
        <Text fx:id="text" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="1"/>
    </children>
</GridPane>
