<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<GridPane fx:controller="multipleDependencies.presentation.ZinsenController" maxHeight="-Infinity" maxWidth="-Infinity"
          minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
          prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Label text="Startkapital">
            <font>
                <Font size="30.0"/>
            </font>
        </Label>
        <Label text="Laufzeit[Jahre]" GridPane.rowIndex="1">
            <font>
                <Font size="30.0"/>
            </font>
        </Label>
        <Label text="Zinssatz" GridPane.rowIndex="2">
            <font>
                <Font size="30.0"/>
            </font>
        </Label>
        <Label text="Endkapital" GridPane.rowIndex="3">
            <font>
                <Font size="30.0"/>
            </font>
        </Label>
        <Text fx:id="textEndkapital" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" GridPane.columnIndex="1"
              GridPane.rowIndex="3">
            <font>
                <Font size="30.0"/>
            </font>
        </Text>
        <TextField fx:id="textFieldKapital" GridPane.columnIndex="1"/>
        <Slider fx:id="sliderLaufzeit" blockIncrement="0.0" majorTickUnit="5.0" max="20.0" minorTickCount="4"
                showTickLabels="true" showTickMarks="true" snapToTicks="true" value="1.0" GridPane.columnIndex="1"
                GridPane.rowIndex="1"/>
        <ChoiceBox fx:id="choiceBoxZinssatz" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
    </children>
</GridPane>
